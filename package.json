{
  "homepage": "https://vikagrabowska.github.io/OctopusProject/",
  "name": "OctopusProject",
  "version": "0.0.2",
  "private": true,
  "type": "module",
  "scripts": {
    "predeploy": "npm run build && npm run copy-readme",
    "deploy": "gh-pages -d dist",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "copy-readme": "cp README.md dist/"
  },
  "dependencies": {
    "opentype.js": "^1.3.4",
    "vue": "^3.4.15",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vue/eslint-config-prettier": "^8.0.0",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "gh-pages": "^6.1.1",
    "node-sass": "^9.0.0",
    "prettier": "^3.0.3",
    "sass": "^1.71.1",
    "sass-loader": "^14.1.1",
    "vite": "^5.0.11"
  }
}
